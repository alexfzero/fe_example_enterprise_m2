{"ast":null,"code":"import logo from \"@/assets/logo.png\";\nexport default {\n  name: 'LoginDialog',\n  props: {\n    isOpened: Boolean\n  },\n  data() {\n    return {\n      logo: logo,\n      user: {\n        username: undefined,\n        password: undefined\n      }\n    };\n  }\n};","map":{"version":3,"mappings":"AAkDA;AAEA;EACAA;EACAC;IACAC;EACA;EACAC;IACA;MACAC;MACAC;QACAC;QACAC;MACA;IACA;EACA;AACA","names":["name","props","isOpened","data","logo","user","username","password"],"sourceRoot":"src/modules/core/components","sources":["login.vue"],"sourcesContent":["<template>\n  <v-dialog\n      :value=\"isOpened\"\n      max-width=\"390\"\n      class=\"justify-center\"\n      persistent\n  >\n    <v-card elevation=\"4\" light tag=\"section\">\n      <v-card-title>\n        <v-layout align-center justify-space-between>\n          <h3 class=\"headline\">\n            Authorization\n          </h3>\n            <v-img alt=\"Authorization\" class=\"ml-3\" contain height=\"48px\" width=\"100px\"\n                   position=\"center right\" :src=\"logo\"></v-img>\n        </v-layout>\n      </v-card-title>\n      <v-divider></v-divider>\n      <v-card-text>\n        <p>Sign in with your username and password:</p>\n        <v-form>\n          <v-text-field\n              outline\n              label=\"Username\"\n              type=\"text\"\n              v-model=\"user.username\"></v-text-field>\n          <v-text-field\n              outline\n              hide-details\n              label=\"Password\"\n              type=\"password\"\n              v-model=\"user.password\"></v-text-field>\n        </v-form>\n      </v-card-text>\n      <v-divider></v-divider>\n      <v-card-actions :class=\"{ 'justify-center pa-3': $vuetify.breakpoint.smAndUp }\">\n        <v-btn\n            color=\"primary\"\n            :large=\"$vuetify.breakpoint.smAndUp\"\n            @click.stop=\"$emit('close', user)\"\n        >\n          <v-icon left>lock</v-icon>\n          Login\n        </v-btn>\n      </v-card-actions>\n    </v-card>\n  </v-dialog>\n</template>\n\n<script>\nimport logo from \"@/assets/logo.png\"\n\nexport default {\n  name: 'LoginDialog',\n  props: {\n    isOpened: Boolean\n  },\n  data() {\n    return {\n      logo: logo,\n      user: {\n        username: undefined,\n        password: undefined\n      }\n    }\n  },\n}\n</script>"]},"metadata":{},"sourceType":"module","externalDependencies":[]}