{"ast":null,"code":"import FormErrorsMixin from \"@/mixins/FormErrorsMixin\";\nimport { makeGetRequest } from \"@/helper\";\nimport urls from \"@/urls/urls\";\nimport roles from \"@/urls/roles\";\nexport default {\n  name: \"PosEditForm\",\n  mixins: [FormErrorsMixin],\n  props: {\n    opened: Boolean,\n    edit: Boolean,\n    posData: Object\n  },\n  data() {\n    return {\n      URL_DEP: urls.API + roles.SUPER_ADMIN + urls.TEMPS.NAME + urls.TEMPS.PATH.DEPARTMENTS,\n      is_head: false,\n      position: undefined,\n      departments: undefined,\n      json_departments: undefined,\n      dep_field: ''\n    };\n  },\n  methods: {\n    getDepartments() {\n      makeGetRequest(this.URL_DEP).then(resp => resp.json()).then(data => {\n        this.departments = data.results;\n        this.json_departments = data;\n      });\n    }\n  },\n  created() {\n    this.getDepartments();\n  }\n};","map":{"version":3,"mappings":"AAkDA;AACA;AACA;AACA;AAEA;EACAA;EACAC;EACAC;IACAC;IACAC;IACAC;EACA;EACAC;IACA;MACAC;MACAC;MACAC;MACAC;MACAC;MACAC;IACA;EACA;EACAC;IACAC;MACAC,6BACAC,0BACAA;QACA;QACA;MACA;IACA;EACA;EACAC;IACA;EACA;AACA","names":["name","mixins","props","opened","edit","posData","data","URL_DEP","is_head","position","departments","json_departments","dep_field","methods","getDepartments","makeGetRequest","then","created"],"sourceRoot":"src/modules/admin/positions","sources":["PosEditForm.vue"],"sourcesContent":["<template>\n  <v-dialog\n      v-model=\"\n      /* eslint-disable-next-line vue/no-mutating-props */\n      opened\"\n      :width=\"width\"\n      :fullscreen=\"$vuetify.breakpoint.smAndDown\"\n      persistent\n      scrollable\n  >\n    <div v-if=\"loading\">\n      <v-text-field\n          v-model=\"position\"\n          label=\"Должность\"\n          :error=\"hasError('position')\"\n          :errorMessages=\"getError('position')\"\n          class=\"mb-n2\"\n      ></v-text-field>\n      <v-autocomplete\n          v-model=\"dep_field\"\n          :items=\"departments\"\n          label=\"Департамент\"\n          :readonly=\"edit\"\n          :disabled=\"edit\"\n          :error=\"hasError('order_type')\"\n          :errorMessages=\"getError('order_type')\"\n      ></v-autocomplete>\n      <v-radio-group\n          v-model=\"is_head\"\n          :error=\"hasError('order_status')\"\n          :error-messages=\"getError('order_status')\"\n      >\n        <v-radio\n            v-if=\"edit\"\n            label=\"Аннулирована\"\n            value=\"true\"\n        ></v-radio>\n        <v-radio\n            v-if=\"edit\"\n            label=\"Отозвана\"\n            value=\"false\"\n        ></v-radio>\n      </v-radio-group>\n    </div>\n  </v-dialog>\n\n\n</template>\n\n<script>\nimport FormErrorsMixin from \"@/mixins/FormErrorsMixin\"\nimport {makeGetRequest} from \"@/helper\";\nimport urls from \"@/urls/urls\";\nimport roles from \"@/urls/roles\";\n\nexport default {\n  name: \"PosEditForm\",\n  mixins: [FormErrorsMixin],\n  props: {\n    opened: Boolean,\n    edit: Boolean,\n    posData: Object,\n  },\n  data() {\n    return {\n      URL_DEP: urls.API + roles.SUPER_ADMIN + urls.TEMPS.NAME + urls.TEMPS.PATH.DEPARTMENTS,\n      is_head: false,\n      position: undefined,\n      departments: undefined,\n      json_departments: undefined,\n      dep_field: ''\n    }\n  },\n  methods: {\n    getDepartments() {\n      makeGetRequest(this.URL_DEP)\n          .then(resp => resp.json())\n          .then(data => {\n            this.departments = data.results\n            this.json_departments = data\n          })\n    }\n  },\n  created() {\n    this.getDepartments()\n  }\n}\n</script>\n\n<style scoped>\n\n</style>"]},"metadata":{},"sourceType":"module","externalDependencies":[]}