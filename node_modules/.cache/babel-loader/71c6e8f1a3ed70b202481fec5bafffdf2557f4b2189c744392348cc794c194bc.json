{"ast":null,"code":"export default {\n  name: 'leftMenu',\n  props: {\n    //! Menu settings\n    isMenuOpen: {\n      type: Boolean,\n      default: true\n    },\n    menuTitle: {\n      type: String,\n      default: 'Enterprise'\n    },\n    menuLogo: {\n      type: String,\n      default: ''\n    },\n    menuIcon: {\n      type: String,\n      default: 'bxl-vuejs'\n    },\n    isPaddingLeft: {\n      type: Boolean,\n      default: true\n    },\n    menuOpenedPaddingLeftBody: {\n      type: String,\n      default: '250px'\n    },\n    menuClosedPaddingLeftBody: {\n      type: String,\n      default: '78px'\n    },\n    //! Menu items\n    menuItems: {\n      type: Array,\n      default: () => [{\n        link: 'OrderList',\n        name: 'Order',\n        tooltip: 'Order',\n        icon: 'bx-credit-card-front'\n      }, {\n        link: 'ContractList',\n        name: 'Contracts',\n        tooltip: 'Contracts',\n        icon: 'bx-file-blank'\n      }, {\n        link: 'FundsList',\n        name: 'Flow funds',\n        tooltip: 'FlowFunds',\n        icon: 'bx-money'\n      }, {\n        link: 'Storage',\n        name: 'Storage',\n        tooltip: 'Storage',\n        icon: 'bx-spreadsheet'\n      }, {\n        link: 'OrgList',\n        name: 'Organizations',\n        tooltip: 'Organizations',\n        icon: 'bx-buildings'\n      }, {\n        link: 'AdminMenu',\n        name: 'Admin',\n        tooltip: 'Admin',\n        icon: 'bx-cog'\n      }]\n    },\n    //! Search\n    isSearch: {\n      type: Boolean,\n      default: true\n    },\n    searchPlaceholder: {\n      type: String,\n      default: 'Search...'\n    },\n    searchTooltip: {\n      type: String,\n      default: 'Search'\n    },\n    //! Styles\n    bgColor: {\n      type: String,\n      default: '#11101d'\n    },\n    secondaryColor: {\n      type: String,\n      default: '#1d1b31'\n    },\n    homeSectionColor: {\n      type: String,\n      default: '#e4e9f7'\n    },\n    logoTitleColor: {\n      type: String,\n      default: '#fff'\n    },\n    iconsColor: {\n      type: String,\n      default: '#fff'\n    },\n    itemsTooltipColor: {\n      type: String,\n      default: '#e4e9f7'\n    },\n    searchInputTextColor: {\n      type: String,\n      default: '#fff'\n    },\n    menuItemsHoverColor: {\n      type: String,\n      default: '#fff'\n    },\n    menuItemsTextColor: {\n      type: String,\n      default: '#fff'\n    },\n    menuFooterTextColor: {\n      type: String,\n      default: '#fff'\n    }\n  },\n  data() {\n    return {\n      isOpened: false,\n      user: {\n        username: 'Guest',\n        authorized: false\n      }\n    };\n  },\n  mounted() {\n    this.isOpened = this.isMenuOpen;\n  },\n  computed: {\n    cssVars() {\n      return {\n        // '--padding-left-body': this.isOpened ? this.menuOpenedPaddingLeftBody : this.menuClosedPaddingLeftBody,\n        '--bg-color': this.bgColor,\n        '--secondary-color': this.secondaryColor,\n        '--home-section-color': this.homeSectionColor,\n        '--logo-title-color': this.logoTitleColor,\n        '--icons-color': this.iconsColor,\n        '--items-tooltip-color': this.itemsTooltipColor,\n        '--serach-input-text-color': this.searchInputTextColor,\n        '--menu-items-hover-color': this.menuItemsHoverColor,\n        '--menu-items-text-color': this.menuItemsTextColor,\n        '--menu-footer-text-color': this.menuFooterTextColor\n      };\n    }\n  },\n  watch: {\n    isOpened() {\n      window.document.body.style.paddingLeft = this.isOpened && this.isPaddingLeft ? this.menuOpenedPaddingLeftBody : this.menuClosedPaddingLeftBody;\n    }\n  }\n};","map":{"version":3,"mappings":"AAiHA;EACAA;EACAC;IACA;IACAC;MACAC;MACAC;IACA;IACAC;MACAF;MACAC;IACA;IACAE;MACAH;MACAC;IACA;IACAG;MACAJ;MACAC;IACA;IACAI;MACAL;MACAC;IACA;IACAK;MACAN;MACAC;IACA;IACAM;MACAP;MACAC;IACA;IACA;IACAO;MACAR;MACAC,gBACA;QACAQ;QACAZ;QACAa;QACAC;MACA,GACA;QACAF;QACAZ;QACAa;QACAC;MACA,GACA;QACAF;QACAZ;QACAa;QACAC;MACA,GACA;QAC