{"ast":null,"code":"import FormErrorsMixin from \"@/mixins/FormErrorsMixin\";\nimport { makeGetRequest } from \"@/helper\";\nimport urls from \"@/urls/urls\";\nimport roles from \"@/urls/roles\";\nimport ScrollCenterDialog from \"@/modules/templates/ScrollCenterDialog.vue\";\nexport default {\n  name: \"PosEditForm\",\n  components: {\n    ScrollCenterDialog\n  },\n  mixins: [FormErrorsMixin],\n  props: {\n    opened: Boolean,\n    edit: Boolean,\n    posData: Object\n  },\n  data() {\n    return {\n      URL_DEP: urls.API + roles.SUPER_ADMIN + urls.TEMPS.NAME + urls.TEMPS.PATH.DEPARTMENTS,\n      width: '400px',\n      loading: false,\n      is_head: false,\n      position: undefined,\n      departments: undefined,\n      json_departments: undefined,\n      dep_field: '',\n      save_loading: false\n    };\n  },\n  methods: {\n    close() {\n      this.$emit('close', false);\n    },\n    save() {\n      console.log('save: asdasd');\n    },\n    getDepartments() {\n      makeGetRequest(this.URL_DEP).then(resp => resp.json()).then(data => {\n        this.departments = data.results;\n        this.json_departments = data;\n      });\n    }\n  },\n  created() {\n    this.getDepartments();\n  },\n  computed: {\n    form_title() {\n      return this.edit === false ? 'Добавить заявку' : 'Редактировать заявку';\n    }\n  }\n};","map":{"version":3,"mappings":"AAkDA;AACA;AACA;AACA;AACA;AAEA;EACAA;EACAC;IAAAC;EAAA;EACAC;EACAC;IACAC;IACAC;IACAC;EACA;EACAC;IACA;MACAC;MACAC;MACAC;MACAC;MACAC;MACAC;MACAC;MACAC;MACAC;IAEA;EACA;EACAC;IACAC;MACA;IACA;IACAC;MACAC;IACA;IACAC;MACAC,6BACAC,0BACAA;QACA;QACA;MACA;IACA;EACA;EACAC;IACA;EACA;EACAC;IACAC;MACA;IACA;EACA;AACA","names":["name","components","ScrollCenterDialog","mixins","props","opened","edit","posData","data","URL_DEP","width","loading","is_head","position","departments","json_departments","dep_field","save_loading","methods","close","save","console","getDepartments","makeGetRequest","then","created","computed","form_title"],"sourceRoot":"src/modules/admin/positions","sources":["PosEditForm.vue"],"sourcesContent":["<template>\n  <scroll-center-dialog\n      v-model=\"\n      /* eslint-disable-next-line vue/no-mutating-props */\n      opened\"\n      :loading=\"save_loading\"\n      width=\"760\"\n      @close=\"close\"\n      @apply=\"save\"\n      :title=\"form_title\"\n      :scrolled=\"false\"\n  >adsadas\n    <div v-if=\"loading\">\n      <v-text-field\n          v-model=\"position\"\n          label=\"Должность\"\n          :error=\"hasError('position')\"\n          :errorMessages=\"getError('position')\"\n          class=\"mb-n2\"\n      ></v-text-field>\n      <v-autocomplete\n          v-model=\"dep_field\"\n          :items=\"departments\"\n          label=\"Департамент\"\n          :readonly=\"edit\"\n          :disabled=\"edit\"\n          :error=\"hasError('order_type')\"\n          :errorMessages=\"getError('order_type')\"\n      ></v-autocomplete>\n      <v-radio-group\n          v-model=\"is_head\"\n          :error=\"hasError('order_status')\"\n          :error-messages=\"getError('order_status')\"\n      >\n        <v-radio\n            v-if=\"edit\"\n            label=\"Аннулирована\"\n            value=\"true\"\n        ></v-radio>\n        <v-radio\n            v-if=\"edit\"\n            label=\"Отозвана\"\n            value=\"false\"\n        ></v-radio>\n      </v-radio-group>\n    </div>\n  </scroll-center-dialog>\n</template>\n\n<script>\nimport FormErrorsMixin from \"@/mixins/FormErrorsMixin\"\nimport {makeGetRequest} from \"@/helper\";\nimport urls from \"@/urls/urls\";\nimport roles from \"@/urls/roles\";\nimport ScrollCenterDialog from \"@/modules/templates/ScrollCenterDialog.vue\";\n\nexport default {\n  name: \"PosEditForm\",\n  components: {ScrollCenterDialog},\n  mixins: [FormErrorsMixin],\n  props: {\n    opened: Boolean,\n    edit: Boolean,\n    posData: Object,\n  },\n  data() {\n    return {\n      URL_DEP: urls.API + roles.SUPER_ADMIN + urls.TEMPS.NAME + urls.TEMPS.PATH.DEPARTMENTS,\n      width: '400px',\n      loading:false,\n      is_head: false,\n      position: undefined,\n      departments: undefined,\n      json_departments: undefined,\n      dep_field: '',\n      save_loading: false,\n\n    }\n  },\n  methods: {\n    close(){\n      this.$emit('close', false)\n    },\n    save(){\n      console.log('save: asdasd')\n    },\n    getDepartments() {\n      makeGetRequest(this.URL_DEP)\n          .then(resp => resp.json())\n          .then(data => {\n            this.departments = data.results\n            this.json_departments = data\n          })\n    },\n  },\n  created() {\n    this.getDepartments()\n  },\n  computed: {\n    form_title() {\n      return this.edit === false ? 'Добавить заявку' : 'Редактировать заявку'\n    },\n  }\n}\n</script>\n\n<style scoped>\n\n</style>"]},"metadata":{},"sourceType":"module","externalDependencies":[]}